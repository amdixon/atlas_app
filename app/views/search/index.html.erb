<div id="container">

<h1>AAATLAS</h1>

<h2>Search City</h2>

<%= form_tag city_index_path, :method => 'get' do %>
  <%= text_field_tag :search, params[:search] %>
  <%= submit_tag "SPIN", :name => nil %>

<% end %>

<br>
<p>Popular Cities</p>
<table>
<% @city.each do |city| %>
  <tr>
    <td><%= city.name %></td>
  </tr>
<% end %>
</table>

</div>